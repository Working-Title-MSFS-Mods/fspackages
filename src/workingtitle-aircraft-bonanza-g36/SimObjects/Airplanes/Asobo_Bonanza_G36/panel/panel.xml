		<Annunciation>
			<Type>Warning</Type>
			<Text>GEAR UP</Text>		
			<Condition>
				<And>
					<Simvar name="IS GEAR RETRACTABLE" unit="Boolean"/>
					<Not>
						<Simvar name="GEAR HANDLE POSITION" unit="Boolean"/>
					</Not>
					<Or>
						<Greater>
							<Simvar name="FLAPS HANDLE INDEX" unit="number"/>
							<Constant>1</Constant>
						</Greater>
						<Equal>
							<Simvar name="GENERAL ENG THROTTLE LEVER POSITION:1" unit="percent"/>
							<Constant>0</Constant>
						</Equal>
					</Or>
				</And>
			</Condition>
		</Annunciation>

		<Annunciation>
			<Type>Caution</Type>
			<Text>LOW VOLTS</Text>
			<Condition>
				<And>
					<Lower>
						<Simvar name="ELECTRICAL MAIN BUS VOLTAGE:1" unit="volts"/>
						<Constant>25</Constant>
					</Lower>
					<Or>
						<Greater>
							<Simvar name="ELECTRICAL BATTERY LOAD:1" unit="amps"/>
							<Constant>0</Constant>
						</Greater>
						<Greater>
							<Simvar name="ELECTRICAL BATTERY LOAD:2" unit="amps"/>
							<Constant>0</Constant>
						</Greater>
					</Or>
				</And>
			</Condition>
		</Annunciation>
		
		<Annunciation>
			<Type>Caution</Type>
			<Text>PITOT FAIL</Text>
			<Condition>
				<Simvar name="PARTIAL PANEL PITOT" unit="Boolean"/>
			</Condition>
		</Annunciation>
		<Annunciation>
			<Type>Caution</Type>
			<Text>PITOT HT OFF</Text>
			<Condition>
				<Not>
					<Simvar name="PITOT HEAT" unit="Boolean"/>
				</Not>
			</Condition>
		</Annunciation>
		
		<Annunciation>
			<Type>Caution</Type>
			<Text>CHECK GEAR</Text>
			<Condition>
				<Or>
					<And>
						<Simvar name="SIM ON GROUND" unit="Boolean"/>
						<Not>
							<Simvar name="GEAR HANDLE POSITION" unit="Boolean"/>
						</Not>
					</And>
					<And>
						<Greater>
							<Simvar name="GEAR CENTER POSITION" unit="Percent"/>
							<Constant>0</Constant>
						</Greater>
						<Lower>
							<Simvar name="GEAR CENTER POSITION" unit="Percent"/>
							<Constant>100</Constant>
						</Lower>
					</And>
					<And>
						<Greater>
							<Simvar name="GEAR LEFT POSITION" unit="Percent"/>
							<Constant>0</Constant>
						</Greater>
						<Lower>
							<Simvar name="GEAR LEFT POSITION" unit="Percent"/>
							<Constant>100</Constant>
						</Lower>
					</And>
					<And>
						<Greater>
							<Simvar name="GEAR RIGHT POSITION" unit="Percent"/>
							<Constant>0</Constant>
						</Greater>
						<Lower>
							<Simvar name="GEAR RIGHT POSITION" unit="Percent"/>
							<Constant>100</Constant>
						</Lower>
					</And>
				</Or>
			</Condition>
		</Annunciation>

	</Annunciations>
	
	  <!-- Voices Alerts -->
  
  <VoicesAlerts>
  
	<Alert>
		<Type>SoundOnly</Type>
		<SoundEvent>aural_500ft</SoundEvent>
		<Condition>
			<StateMachine>
				<State id="Under" value="0">
					<Transition to="Over">
						<GreaterEqual>
							<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
							<Constant>510</Constant>
						</GreaterEqual>
					</Transition>
				</State>
				<State id="Over" value="0">
					<Transition to="Alerting">
						<LowerEqual>
							<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
							<Constant>500</Constant>
						</LowerEqual>
					</Transition>
				</State>
				<State id="Alerting" value="1">
					<Transition to="Under">
						<Constant>1</Constant>
					</Transition>
				</State>
			</StateMachine>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>SoundOnly</Type>
		<SoundEvent>aural_stall</SoundEvent>
		<Condition>
			<Simvar name="STALL WARNING" unit="Bool"/>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>Warning</Type>
		<ShortText>PULL UP</ShortText>
		<LongText>PULL UP</LongText>
		<SoundEvent>aural_pull_up</SoundEvent>
		<Condition>
			<And>
				<LinearMultiPoint>
					<Table>
						<ReferencePoints>1160,2320,4930,12000</ReferencePoints>
						<Maximums>0,1070,2380,4285</Maximums>
					</Table>
					<Param>
						<Multiply>
							<Simvar name="VERTICAL SPEED" unit="feet per minute"/>
							<Constant>-1</Constant>
						</Multiply>
					</Param>
					<Param>
						<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
					</Param>
				</LinearMultiPoint>
				<Not>
					<Simvar name="SIM ON GROUND" unit="bool"/>
				</Not>
			</And>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>SoundOnly</Type>
		<SoundEvent>aural_overspeed</SoundEvent>
		<Condition>
			<Simvar name="OVERSPEED WARNING" unit="bool"/>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>Caution</Type>
		<ShortText>TERRAIN</ShortText>
		<LongText>SINK RATE</LongText>
		<SoundEvent>aural_sink_rate</SoundEvent>
		<Condition>
				<And>
					<LinearMultiPoint>
						<Table>
							<ReferencePoints>1160,2320,4930,12000</ReferencePoints>
							<Minimums>0,1070,2380,4285</Minimums>
							<Maximums>0,1460,2980,5360</Maximums>
						</Table>
						<Param>
							<Multiply>
								<Simvar name="VERTICAL SPEED" unit="feet per minute"/>
								<Constant>-1</Constant>
							</Multiply>
						</Param>
						<Param>
							<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
						</Param>
					</LinearMultiPoint>
					<Not>
						<Simvar name="SIM ON GROUND" unit="bool"/>
					</Not>
				</And>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>Caution</Type>
		<ShortText>TERRAIN</ShortText>
		<LongText>DONT' SINK</LongText>
		<SoundEvent>aural_dont_sink</SoundEvent>
		<Condition>
			<And>
				<Lower>
					<DistanceFromOrigin/>
					<Constant>2</Constant>
				</Lower>
				<Lower>
					<HeadingChangeFromDeparture/>
					<Constant>110</Constant>
				</Lower>
				<Or>
					<LinearMultiPoint>
						<Table>
							<ReferencePoints>0,500,7000</ReferencePoints>
							<Maximums>50,700,700</Maximums>
								<Minimums>50,50,50</Minimums>
						</Table>
						<Param>
							<Multiply>
								<Simvar name="VERTICAL SPEED" unit="feet per minute"/>
								<Constant>-1</Constant>
							</Multiply>
						</Param>
						<Param>
							<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
						</Param>
					</LinearMultiPoint>
					<LinearMultiPoint>
						<Table>
							<ReferencePoints>10,70,150</ReferencePoints>
							<Maximums>50,700,700</Maximums>
								<Minimums>50,50,50</Minimums>
						</Table>
						<Param>
							<Substract>
								<MaxSinceStart>
									<Simvar name="PLANE ALTITUDE" unit="feet"/>
								</MaxSinceStart>
								<Simvar name="PLANE ALTITUDE" unit="feet"/>
							</Substract>
						</Param>
						<Param>
							<Simvar name="PLANE ALT ABOVE GROUND" unit="feet"/>
						</Param>
					</LinearMultiPoint>
				</Or>
			</And>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>SoundOnly</Type>
		<SoundEvent>aural_landing_gear</SoundEvent>
		<Condition>
			<And>
				<Simvar name="IS GEAR RETRACTABLE" unit="Boolean"/>
				<Not>
					<Simvar name="GEAR HANDLE POSITION" unit="Boolean"/>
				</Not>
				<Or>
					<Greater>
						<Simvar name="FLAPS HANDLE INDEX" unit="number"/>
						<Constant>1</Constant>
					</Greater>
					<Equal>
						<Simvar name="GENERAL ENG THROTTLE LEVER POSITION:1" unit="percent"/>
						<Constant>0</Constant>
					</Equal>
				</Or>
			</And>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>Test</Type>
		<ShortText>TAWS TEST</ShortText>
		<Condition>
			<And>
				<Lower>
					<TimeSinceStart/>
					<Constant>30000</Constant>
				</Lower>
				<InstrumentWasOff/>
			</And>
		</Condition>
	</Alert>
	
	<Alert>
		<Type>SoundOnly</Type>
		<SoundEvent>aural_taws_system_test_ok</SoundEvent>
		<Condition>
			<And>
				<Greater>
					<TimeSinceStart/>
					<Constant>30000</Constant>
				</Greater>
				<InstrumentWasOff/>
			</And>
		</Condition>
		<Once>True</Once>
	</Alert>

  </VoicesAlerts>
	
