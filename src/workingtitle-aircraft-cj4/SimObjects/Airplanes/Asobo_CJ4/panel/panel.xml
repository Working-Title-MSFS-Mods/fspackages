<PlaneHTMLConfig>
	<Instrument>
		<Name>HourMeter</Name>
		<Electric>
			<Simvar name="CIRCUIT GENERAL PANEL ON" unit="Boolean"/>
		</Electric>
	</Instrument>
	<Instrument>
		<Name>CJ4_PFD</Name>
		<RadarAltitude>True</RadarAltitude>
		<Electric>
			<Or>
				<Simvar name="CIRCUIT ON:39" unit="Boolean"/>
				<Simvar name="CIRCUIT ON:40" unit="Boolean"/>
			</Or>
		</Electric>
	</Instrument>
	<Instrument>
		<Name>CJ4_MFD</Name>
		<Electric>
			<Or>
				<Simvar name="CIRCUIT ON:41" unit="Boolean"/>
				<Simvar name="CIRCUIT ON:42" unit="Boolean"/>
			</Or>
		</Electric>
	</Instrument>
	<Instrument>
		<Name>CJ4_SAI</Name>
		<Electric>
			<Simvar name="CIRCUIT ON:49" unit="Boolean"/>
		</Electric>
	</Instrument>
	<Instrument>
		<Name>CJ4_FMC</Name>
		<Electric>
			<Or>
				<Simvar name="CIRCUIT ON:47" unit="Boolean"/>
				<Simvar name="CIRCUIT ON:48" unit="Boolean"/>
			</Or>
		</Electric>
		<AlwaysUpdate>True</AlwaysUpdate>
	</Instrument>
	
	<!-- Annunciations -->
	
	<Annunciations>
		<Annunciation>
		  <Type>Warning</Type>
		  <Text>FUEL OFF</Text>
		  <Condition>
			<Equal>
			  <Simvar name="FUEL TANK SELECTOR:1" unit="number"/>
			  <Constant>0</Constant>
			</Equal>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Warning</Type>
		  <Text>FUEL PRESS</Text>
		  <Condition>
			<LowerEqual>
			  <Simvar name="GENERAL ENG FUEL PRESSURE:1" unit="psi"/>
			  <Constant>10</Constant>
			</LowerEqual>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Warning</Type>
		  <Text>OIL PRESS</Text>
		  <Condition>
			<LowerEqual>
			  <Simvar name="ENG OIL PRESSURE:1" unit="psi"/>
			  <Constant>25</Constant>
			</LowerEqual>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Warning</Type>
		  <Text>ITT</Text>
		  <Condition>
			<Or>
			  <Timer time="30">
				<Greater>
					<Simvar name="TURB ENG ITT:1" unit="celsius"/>
					<Constant>1100</Constant>
				</Greater>
			  </Timer>
			  <Timer time="60">
				<Greater>
					<Simvar name="TURB ENG ITT:1" unit="celsius"/>
					<Constant>970</Constant>
				</Greater>
			  </Timer>
			  <Timer time="90">
				<Greater>
					<Simvar name="TURB ENG ITT:1" unit="celsius"/>
					<Constant>940</Constant>
				</Greater>
			  </Timer>
			</Or>
		  </Condition>
		</Annunciation>
		
		<Annunciation>
		  <Type>Warning</Type>
		  <Text>CABIN ALTITUDE</Text>
		  <Condition>
			<Greater>
			  <Simvar name="PRESSURIZATION CABIN ALTITUDE" unit="feet"/>
			  <Constant>45000</Constant>
			</Greater>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Warning</Type>
		  <Text>CABIN DIFF PRESS</Text>
		  <Condition>
			<Greater>
			  <Simvar name="PRESSURIZATION PRESSURE DIFFERENTIAL" unit="psi"/>
			  <Constant>11.2</Constant>
			</Greater>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Warning</Type>
		  <Text>DOOR</Text>
		  <Condition>
			<Greater>
			  <Simvar name="EXIT OPEN:0" unit="percent"/>
			  <Constant>0</Constant>
			</Greater>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Warning</Type>
		  <Text>GEAR UNSAFE</Text>
		  <Condition>
			<And>
				<Not>
					<Simvar name="GEAR HANDLE POSITION" unit="Bool"/>
				</Not>
				<Simvar name="SIM ON GROUND" unit="Bool"/>
			</And>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Warning</Type>
		  <Text>FIRE</Text>
		  <Condition>
			<Or>
				<Simvar name="ENG ON FIRE:1" unit="Bool"/>
				<Simvar name="ENG ON FIRE:2" unit="Bool"/>
			</Or>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>PARKING BRAKE</Text>
		  <Condition>
			<And>
				<Greater>
					<Simvar name="TURB ENG N1:1" unit="percent"/>
					<Constant>70</Constant>
				</Greater>
				<And>
					<Simvar name="BRAKE PARKING INDICATOR" unit="Bool"/>
				</And>
			</And>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>OIL PRESS</Text>
		  <Condition>
			<Or>
			  <GreaterEqual>
				<Simvar name="ENG OIL PRESSURE:1" unit="psi"/>
				<Constant>105</Constant>
			  </GreaterEqual>
			  <LowerEqual>
				<Simvar name="ENG OIL PRESSURE:1" unit="psi"/>
				<Constant>25</Constant>
			  </LowerEqual>
			</Or>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>AUX BOOST PMP ON</Text>
		  <Condition>
			<Or>
				<Simvar name="GENERAL ENG FUEL PUMP ON:1" unit="Bool"/>
				<Simvar name="GENERAL ENG FUEL PUMP ON:2" unit="Bool"/>
			</Or>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>FUEL LOW </Text>
		  <Condition Suffix="L-R">
			<And>
			  <Lower>
				<Simvar name="FUEL TANK LEFT MAIN QUANTITY" unit="gallon"/>
				<Constant>9</Constant>
			  </Lower>
			  <Lower>
				<Simvar name="FUEL TANK RIGHT MAIN QUANTITY" unit="gallon"/>
				<Constant>9</Constant>
			  </Lower>
			</And>
		  </Condition>
		  <Condition Suffix="L">
			<Lower>
			  <Simvar name="FUEL TANK LEFT MAIN QUANTITY" unit="gallon"/>
			  <Constant>9</Constant>
			</Lower>
		  </Condition>
		  <Condition Suffix="R">
			<Lower>
			  <Simvar name="FUEL TANK RIGHT MAIN QUANTITY" unit="gallon"/>
			  <Constant>9</Constant>
			</Lower>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>FUEL IMBALANCE</Text>
		  <Condition>
			<Inequal tolerance="15">
				<Simvar name="FUEL TANK LEFT MAIN QUANTITY" unit="gallon"/>
				<Simvar name="FUEL TANK RIGHT MAIN QUANTITY" unit="gallon"/>
			</Inequal>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>BAT OFF</Text>
		  <Condition>
		  <Not>
			<Simvar name="ELECTRICAL MASTER BATTERY" unit="Bool"/>
		  </Not>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>BAT AMP</Text>
		  <Condition>
			<Greater>
				<Simvar name="ELECTRICAL BATTERY BUS AMPS" unit="amperes"/>
				<Constant>25</Constant>
			</Greater>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>MAIN GEN</Text>
		  <Condition>
			<And>
				<Not>
					<Simvar name="GENERAL ENG MASTER ALTERNATOR:1" unit="Bool"/>
				</Not>
				<Not>
					<Simvar name="GENERAL ENG MASTER ALTERNATOR:2" unit="Bool"/>
				</Not>
			</And>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>LOW VOLTAGE</Text>
		  <Condition>
			<Lower>
				<Simvar name="ELECTRICAL MAIN BUS VOLTAGE" unit="volts"/>
				<Constant>24.5</Constant>
			</Lower>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>VACUUM LOW</Text>
		  <Condition>
			<Equal>
				<Simvar name="PARTIAL PANEL VACUUM" unit="Enum"/>
				<Constant>1</Constant>
			</Equal>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Caution</Type>
		  <Text>PITOT/STATIC COLD </Text>
		  <Condition Suffix="BOTH">
			<Or>
				<And>
					<Not>
						<Simvar name="SIM ON GROUND" unit="bool"/>
					</Not>
					<Not>
						<Or>
							<Simvar name="L:XMLVAR_Pitot_1" unit="Bool"/>
							<Simvar name="L:XMLVAR_Pitot_2" unit="Bool"/>
						</Or>
					</Not>
				</And>
				<And>
					<Greater>
						<Simvar name="TURB ENG N1:1" unit="percent"/>
						<Constant>70</Constant>
					</Greater>
					<Not>
						<Or>
							<Simvar name="L:XMLVAR_Pitot_1" unit="Bool"/>
							<Simvar name="L:XMLVAR_Pitot_2" unit="Bool"/>
						</Or>
					</Not>
				</And>
			</Or>
		  </Condition>
		  <Condition Suffix="L">
			<Or>
				<And>
					<Not>
						<Simvar name="SIM ON GROUND" unit="bool"/>
					</Not>
					<Not>
						<Or>
							<Simvar name="L:XMLVAR_Pitot_1" unit="Bool"/>
						</Or>
					</Not>
				</And>
				<And>
					<Greater>
						<Simvar name="TURB ENG N1:1" unit="percent"/>
						<Constant>70</Constant>
					</Greater>
					<Not>
						<Or>
							<Simvar name="L:XMLVAR_Pitot_1" unit="Bool"/>
						</Or>
					</Not>
				</And>
			</Or>
		  </Condition>
		  <Condition Suffix="R">
			<Or>
				<And>
					<Not>
						<Simvar name="SIM ON GROUND" unit="bool"/>
					</Not>
					<Not>
						<Or>
							<Simvar name="L:XMLVAR_Pitot_2" unit="Bool"/>
						</Or>
					</Not>
				</And>
				<And>
					<Greater>
						<Simvar name="TURB ENG N1:1" unit="percent"/>
						<Constant>70</Constant>
					</Greater>
					<Not>
						<Or>
							<Simvar name="L:XMLVAR_Pitot_2" unit="Bool"/>
						</Or>
					</Not>
				</And>
			</Or>
		  </Condition>
		</Annunciation>
		
		<Annunciation>
		  <Type>Caution</Type>
		  <Text>NG HI</Text>
		  <Condition>
			<Or>
				<Greater>
					<Simvar name="TURB ENG N1:1" unit="percent"/>
					<Constant>103</Constant>
				</Greater>
				<Greater>
					<Simvar name="TURB ENG N1:2" unit="percent"/>
					<Constant>103</Constant>
				</Greater>
			</Or>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Advisory</Type>
		  <Text>MAX DIFF MODE</Text>
		  <Condition>
			<Equal>
				<Simvar name="BLEED AIR SOURCE CONTROL" unit="Enum"/>
				<Constant>3</Constant>
			</Equal>
		  </Condition>
		</Annunciation>

		<Annunciation>
		  <Type>Advisory</Type>
		  <Text>STARTER</Text>
		  <Condition>
			<Or>
				<Simvar name="GENERAL ENG STARTER ACTIVE:1" unit="Bool"/>
				<Simvar name="GENERAL ENG STARTER ACTIVE:2" unit="Bool"/>
			</Or>
		  </Condition>
		</Annunciation>
		
	</Annunciations>
  
	<!-- Voices Alerts -->
  
	<VoicesAlerts>
	
		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>WT_aural_500ft</SoundEvent>
			<Condition>
				<StateMachine>
					<State id="Under" value="0">
						<Transition to="Over">
							<GreaterEqual>
								<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
								<Constant>510</Constant>
							</GreaterEqual>
						</Transition>
					</State>
					<State id="Over" value="0">
						<Transition to="Alerting">
							<LowerEqual>
								<Simvar name="PLANE ALT ABOVE GROUND MINUS CG" unit="feet"/>
								<Constant>500</Constant>
							</LowerEqual>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="Under">
							<Constant>1</Constant>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Alert>

		<Alert>
			<Type>SoundOnly</Type>
			<SoundEvent>WT_aural_minimums</SoundEvent>
			<Condition>
				<StateMachine>
					<State id="Under" value="0">
						<Transition to="Over">
							<GreaterEqual>
								<Simvar name="INDICATED ALTITUDE" unit="feet"/>
								<Simvar name="L:WT_CJ4_BARO_SET" unit="number"/>
							</GreaterEqual>
						</Transition>
					</State>
					<State id="Over" value="0">
						<Transition to="Alerting">
							<LowerEqual>
								<Simvar name="INDICATED ALTITUDE" unit="feet"/>
								<Simvar name="L:WT_CJ4_BARO_SET" unit="number"/>
							</LowerEqual>
						</Transition>
					</State>
					<State id="Alerting" value="1">
						<Transition to="Under">
							<Constant>1</Constant>
						</Transition>
					</State>
				</StateMachine>
			</Condition>
		</Alert>

