            </Page>

            <Page SubjectTT="AFTER TAKEOFF/CLIMB">
                <Checkpoint ReferenceId="CJ4_LANDING_GEAR_UP"/>
                <Checkpoint ReferenceId="CJ4_FLAPS_SET_0">
                    <Clue name="Clear of obstacles and V2+10"/>
                </Checkpoint>
                <Checkpoint ReferenceId="CJ4_THROTTLES_CLB"/>
                <Checkpoint ReferenceId="CJ4_YAW_DAMPER_ON"/>
                <Checkpoint ReferenceId="CJ4_AUTOPILOT_ASREQUIRED"/>
                <Checkpoint ReferenceId="CJ4_ICE_PROTECTION_SYSTEMS_FLIGHT"/>
                <Checkpoint ReferenceId="CJ4_PASSENGER_LIGHTS_ASREQUIRED"/>
                <Checkpoint ReferenceId="CJ4_LANDING_LIGHTS_ASREQUIRED"/>
                <Checkpoint ReferenceId="CJ4_PRESSURIZATION_CHECK"/>
                <Checkpoint ReferenceId="CJ4_ALTIMETERS_STD"/>
            </Page>
        </Step>

        <Step ChecklistStepId="FLIGHT_CRUISE">
            <Page SubjectTT="CRUISE">
                <Checkpoint ReferenceId="CJ4_THROTTLES_ASREQUIRED"/>
                <Checkpoint ReferenceId="CJ4_PRESSURIZATION_CHECK"/>
                <Checkpoint ReferenceId="CJ4_ICE_PROTECTION_SYSTEMS_FLIGHT"/>
            </Page>
        </Step>

        <Step ChecklistStepId="FLIGHT_DESCENT">
            <Page SubjectTT="DESCENT">
                <Checkpoint ReferenceId="CJ4_PRESSURIZATION_SET"/>
                <Checkpoint ReferenceId="CJ4_ICE_PROTECTION_SYSTEMS_FLIGHT"/>
                <Checkpoint ReferenceId="CJ4_ALTIMETER_CHECK_DESCENT"/>
                <Checkpoint ReferenceId="CJ4_CDU_XPRD_CHECK"/>
                <Checkpoint ReferenceId="CJ4_EXTERIOR_LIGHTS_DESCENT"/>
            </Page>
        </Step>

        <Step ChecklistStepId="LANDING_APPROACH">
            <Page SubjectTT="APPROACH">
                <Checkpoint ReferenceId="CJ4_LANDING_DATA"/>
                <Checkpoint ReferenceId="CJ4_CREW_BRIEFING"/>
                <Checkpoint ReferenceId="CJ4_AVIONICS_INSTRUMENTS_CHECK"/>
                <Checkpoint ReferenceId="CJ4_MINIMUMS_SET"/>
                <Checkpoint ReferenceId="CJ4_FUEL_TRANSFER_OFF"/>
                <Checkpoint ReferenceId="CJ4_EXTERIOR_LIGHTS_APPROACH"/>
                <Checkpoint ReferenceId="CJ4_ICE_PROTECTION_SYSTEMS_FLIGHT"/>
                <Checkpoint ReferenceId="CJ4_FLAPS_SET_15"/>
                <Checkpoint ReferenceId="CJ4_PASSENGER_BRIEFING"/>
                <Checkpoint ReferenceId="CJ4_SEATS_UP"/>
                <Checkpoint ReferenceId="CJ4_SEATS_BELTS"/>
                <Checkpoint ReferenceId="CJ4_PASSENGER_LIGHTS_SAFETY_ON"/>
                <Checkpoint ReferenceId="CJ4_EICAS_CHECK"/>
                <Checkpoint ReferenceId="CJ4_PRESSURIZATION_SET_APPROACH"/>
            </Page>

            <Page SubjectTT="BEFORE LANDING">
                <Checkpoint ReferenceId="CJ4_LANDING_GEAR_DOWN"/>
                <Checkpoint ReferenceId="CJ4_FLAPS_SET_35"/>
                <Checkpoint ReferenceId="CJ4_SPEEDBRAKES_DISENGAGE"/>
                <Checkpoint ReferenceId="CJ4_AIRSPEED_VREF"/>
                <Checkpoint ReferenceId="CJ4_DISENGAGE_AUTOPILOT_AND_YAW"/>
            </Page>
        </Step>

        <Step ChecklistStepId="LANDING_TOUCHDOWN">
            <Page SubjectTT="LANDING">
                <Checkpoint ReferenceId="CJ4_THROTTLES_IDLE"/>
                <Checkpoint ReferenceId="CJ4_BRAKES_APPLY_TOUCHDOWN"/>
                <Checkpoint ReferenceId="CJ4_GROUND_SPOILERS_ENGAGE"/>
            </Page>

            <!-- In this step to keep manual's checklist order -->
            <Page SubjectTT="ALL ENGINE GO-AROUND">
                <Checkpoint ReferenceId="CJ4_GA_BUTTON"/>
                <Checkpoint ReferenceId="CJ4_THROTTLES_TO"/>
                <Checkpoint ReferenceId="CJ4_PITCH_ALTITUDE_GOAROUND"/>
                <Checkpoint ReferenceId="CJ4_FLAPS_SET_15"/>
                <Checkpoint ReferenceId="CJ4_LANDING_GEAR_UP"/>
                <Checkpoint ReferenceId="CJ4_FLAPS_UP_GOAROUND"/>
                <Checkpoint ReferenceId="CJ4_AIRSPEED_ASREQUIRED"/>
                <Checkpoint ReferenceId="CJ4_THROTTLES_ASREQUIRED"/>
                <Checkpoint ReferenceId="CJ4_YAW_DAMPER_ON"/>
                <Checkpoint ReferenceId="CJ4_AUTOPILOT_ASREQUIRED"/>
            </Page>
        </Step>

        <Step ChecklistStepId="LANDING_GROUNDROLL">
            <Page SubjectTT="AFTER LANDING">
                <Checkpoint ReferenceId="CJ4_SPEEDBRAKES_DISENGAGE"/>
                <Checkpoint ReferenceId="CJ4_FLAPS_SET_0"/>
                <Checkpoint ReferenceId="CJ4_ICE_PROTECTION_SYSTEMS_AFTER_LANDING"/>
                <Checkpoint ReferenceId="CJ4_RADAR_STBY"/>
                <Checkpoint ReferenceId="CJ4_EXTERIOR_LIGHTS_TAXI"/>
            </Page>
        </Step>

        <Step ChecklistStepId="LANDING_TAXI_IN">
        </Step>

        <Step ChecklistStepId="LANDING_GATE">
            <Page SubjectTT="Shutdown">
                <Checkpoint ReferenceId="CJ4_PARKING_BRAKE_SET"/>
                <Checkpoint ReferenceId="CJ4_EMERGENCY_LIGHTS_SWITCH_OFF"/>
                <Checkpoint ReferenceId="CJ4_STANDBY_FLIGHT_DISPLAY_SWITCH_OFF"/>
                <Checkpoint ReferenceId="CJ4_AVIONICS_SWITCH_OFF"/>
                <Checkpoint ReferenceId="CJ4_ICE_PROTECTION_SYSTEMS_OFF"/>
                <Checkpoint ReferenceId="CJ4_THROTTLES_IDLE"/>
                <Checkpoint ReferenceId="CJ4_ENGINES_SHUTDOWN">
                    <Clue name="Engine should be at ground idle for at least 2 minutes prior to shutdown to allow hot section temperatures to equalize"/>
                </Checkpoint>
                <Checkpoint ReferenceId="CJ4_EXTERIOR_LIGHTS_OFF"/>
                <Checkpoint ReferenceId="CJ4_BATTERY_SWITCH_OFF"/>
                <Checkpoint ReferenceId="CJ4_CONTROL_LOCK"/>
                <Checkpoint ReferenceId="CJ4_OIL_LEVEL_CHECK"/>
                <Checkpoint ReferenceId="CJ4_INSTALL_ENGINE_COVERS"/>
            </Page>
        </Step>
    </Checklist.Checklist>
</SimBase.Document>
