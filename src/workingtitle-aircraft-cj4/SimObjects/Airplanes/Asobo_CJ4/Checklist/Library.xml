            <Instrument Id="HANDLING_LEVER_FLAPS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_FLAPS_SET_35">
            <CheckpointDesc SubjectTT="Flaps" ExpectationTT="35 deg"/>
            <Instrument Id="HANDLING_LEVER_FLAPS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_FLAPS_GOAROUND">
