        <Checkpoint Id="CJ4_SEATS_BELTS">
            <CheckpointDesc SubjectTT="Seat belts/Shoulder harnesses" ExpectationTT="Adjust/Secure"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_SEATS_BELTS_PEDALS_ADJUST">
            <CheckpointDesc SubjectTT="Seats/Belts/Harnesses/Pedals" ExpectationTT="Adjust/Secure"/>
        </Checkpoint>
