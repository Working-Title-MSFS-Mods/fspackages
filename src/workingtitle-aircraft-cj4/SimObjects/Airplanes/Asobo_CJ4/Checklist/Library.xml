            <Instrument Id="LANDING_GEAR_LEVER_GEAR"/>
            <Instrument Id="LANDING_GEAR_LIGHT"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_LANDING_GEAR_DOWN">
            <CheckpointDesc SubjectTT="Landing gear (positive climb)" ExpectationTT="Down (3 Green)"/>
            <ForceCamera name="Instrument01"/>
            <Instrument Id="LANDING_GEAR_LEVER_GEAR"/>
            <Instrument Id="LANDING_GEAR_LIGHT"/>
