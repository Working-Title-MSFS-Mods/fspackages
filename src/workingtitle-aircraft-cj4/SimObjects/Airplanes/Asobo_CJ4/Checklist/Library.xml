            <CheckpointDesc SubjectTT="BATT DISC Button" ExpectationTT="DISC"/>
            <Clue name="No displays powered&#xA;(External power MUST be disconnected)"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_DISC_TEST_2">
            <CheckpointDesc SubjectTT="BATT DISC Button" ExpectationTT="NORM"/>
            <Clue name="Minimum BATT VOLT 24&#xA;(External power MUST be disconnected)"/>
