        <!--  -->
        <!-- DEICE -->
        <!--  -->
        <Checkpoint Id="CJ4_ICE_PROTECTION_SYSTEMS_OFF">
            <CheckpointDesc SubjectTT="Ice protection system" ExpectationTT="OFF"/>
            <ForceCamera name="Instrument02"/>
            <Instrument Id="DEICE_SWITCH_PITOTHEAT"/>
            <Instrument Id="DEICE_ENGINE"/>
            <Instrument Id="DEICE_ENGINE1"/>
            <Instrument Id="DEICE_ENGINE2"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ICE_PROTECTION_SYSTEMS_FLIGHT">
            <CheckpointDesc SubjectTT="Ice protection system" ExpectationTT="As required"/>
            <Clue name="Do not operate TAIL DEICE when SAT is below -30deg C"/>
            <ForceCamera name="Instrument02"/>
            <!-- TODO: Fix highlighting for all ice protection buttons and add them here -->
        </Checkpoint>
        <Checkpoint Id="CJ4_PITOT_HEAT_ON">
            <CheckpointDesc SubjectTT="Pitot/Static heat buttons" ExpectationTT="ON"/>
            <Clue name="Limit ground operation of pitot/static heat to 2 minutes to prevent damage to the pitot/static heaters"/>
            <ForceCamera name="Instrument02"/>
            <Instrument Id="DEICE_SWITCH_PITOTHEAT"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ENGINE_ONLY_ANTI_ICE_ASREQUIRED">
            <CheckpointDesc SubjectTT="ENGINE ONLY ANTI-ICE buttons" ExpectationTT="As required"/>
            <ForceCamera name="Instrument02"/>
            <Instrument Id="DEICE_ENGINE"/>
            <Instrument Id="DEICE_ENGINE1"/>
            <Instrument Id="DEICE_ENGINE2"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ICE_PROTECTION_SYSTEMS_AFTER_LANDING">
            <CheckpointDesc SubjectTT="Ice protection systems" ExpectationTT="OFF"/>
            <Clue name="PITOT heat - OFF, WING/ENG heat - OFF, TAIL heat - OFF&#xA;ENG ONLY heat - as requred"/>
            <ForceCamera name="Instrument02"/>
            <Instrument Id="DEICE_SWITCH_PITOTHEAT"/>
            <!-- TODO: Fix highlighting for all ice protection buttons and add them here -->
        </Checkpoint>
        <Checkpoint Id="CJ4_ICE_PROTECTION_SYSTEMS_BEFORE_TO">
            <CheckpointDesc SubjectTT="Ice Protection systems" ExpectationTT="As Required"/>
            <Clue name="Do not operate WING/ENG ANTI-ICE on the ground at high engine rpm after anti-ice messages have cleared.&#xA;Do not operate TAIL DEICE when SAT is below -30deg C"/>
            <ForceCamera name="Instrument02"/>
            <!-- TODO: Fix highlighting for all ice protection buttons and add them here -->
        </Checkpoint>
        <Checkpoint Id="CJ4_PITOT_TEST">
            <ForceCamera name="Instrument02"/>
            <CheckpointDesc SubjectTT="PITOT/STATIC HEAT Buttons (both)" ExpectationTT="On/CAS Message Clears then After 30 Seconds, Off"/>
        </Checkpoint>

        <!--  -->
        <!-- FLIGHT CONTROLS -->
        <!--  -->
        <Checkpoint Id="CJ4_ROTATE">
            <CheckpointDesc SubjectTT="Control wheel" ExpectationTT="Rotate at vR"/>
            <Clue name="To achieve 10deg initial pitch&#xA;Use flight director TO mode"/>
            <Instrument Id="HANDLING_Yoke"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ENGINES_SHUTDOWN">
            <CheckpointDesc SubjectTT="ENGINE RUN/STOP buttons" ExpectationTT="STOP"/>
            <Clue name="Engine should be at ground idle for at least 2 minutes prior to shutdown to allow hot section temperatures to equalize"/>
            <ForceCamera name="Instrument05"/>
            <Instrument id="FUEL_RUNSTOP_2"/>
            <Instrument id="FUEL_PUSH_RUNSTOP1"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_LANDING_GEAR_UP">
            <CheckpointDesc SubjectTT="Landing gear (positive climb)" ExpectationTT="UP"/>
