            <Instrument Id="ELECTRICAL_SWITCH_BATTERY_MASTER"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_EMERGENCY_LIGHTS_SWITCH_ARMED">
            <CheckpointDesc SubjectTT="EMERGENCY LIGHTS switch" ExpectationTT="ARMED"/>
