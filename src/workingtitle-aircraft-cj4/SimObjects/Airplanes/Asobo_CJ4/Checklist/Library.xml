        </Checkpoint>
        <Checkpoint Id="CJ4_BRAKES_HOLD">
            <CheckpointDesc SubjectTT="Brakes" ExpectationTT="Apply/Hold"/>
            <Instrument Id="HANDLING_RudderPedals"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_BRAKES_APPLY_TOUCHDOWN">
            <CheckpointDesc SubjectTT="Brakes (after nose touchdown)" ExpectationTT="Apply"/>
            <Instrument Id="HANDLING_RudderPedals"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_BRAKES_RELEASE">
            <CheckpointDesc SubjectTT="Brakes" ExpectationTT="Release"/>
            <Instrument Id="HANDLING_RudderPedals"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_BRAKES_CHECK">
            <CheckpointDesc SubjectTT="Brakes" ExpectationTT="Check"/>
            <Clue name="If a no braking condition is encountered during taxi, use the emergency brake. If the emergency brake is used, maintenance is required before flight"/>
