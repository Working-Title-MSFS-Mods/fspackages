        </Checkpoint>
        <Checkpoint Id="CJ4_RUDDER_BIAS_SYSTEMS_CHECK">
            <CheckpointDesc SubjectTT="Rudder bias system" ExpectationTT="Check"/>
        </Checkpoint>
