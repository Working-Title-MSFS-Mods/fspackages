            <Instrument Id="ELECTRICAL_Switch_Avionics_Master"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_EMERGENCY_LIGHTS_SWITCH_OFF">
            <CheckpointDesc SubjectTT="EMERGENCY LIGHTS switch" ExpectationTT="OFF"/>
            <ForceCamera name="Instrument02"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_STANDBY_FLIGHT_DISPLAY_SWITCH_OFF">
            <CheckpointDesc SubjectTT="STANDBY FLIGHT DISPLAY switch" ExpectationTT="ON"/>
            <ForceCamera name="Instrument02"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ELECTRICAL_SYSTEM_CHECK">
            <CheckpointDesc SubjectTT="Electrical system" ExpectationTT="Check"/>
            <Clue name="If both engines running"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ELECTRICAL_SWITCHES_ALL_OFF">
            <ForceCamera name="Instrument02"/>
            <CheckpointDesc SubjectTT="ELECTRICAL POWER Panel Switches (all)" ExpectationTT="OFF"/>
            <ForceCamera name="Instrument02"/>
            <Instrument Id="ELECTRICAL_SWITCH_ALTERNATOR"/>
            <Instrument Id="ELECTRICAL_SWITCH_BATTERY_MASTER"/>
            <Instrument Id="ELECTRICAL_SWITCH_AVIONICS_MASTER"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_INT_MASTER_ON">
            <CheckpointDesc SubjectTT="INT MASTER Button" ExpectationTT="NORM"/>
        </Checkpoint>
