            <ForceCamera name="Instrument05"/>
            <Instrument Id="ENGINE_LEVER_THROTTLE1"/>
            <Instrument Id="ENGINE_LEVER_THROTTLE2"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_THROTTLES_CLB">
            <CheckpointDesc SubjectTT="Throttles" ExpectationTT="CLB"/>
            <ForceCamera name="Instrument05"/>
            <Instrument Id="ENGINE_LEVER_THROTTLE1"/>
            <Instrument Id="ENGINE_LEVER_THROTTLE2"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_THROTTLES_ASREQUIRED">
            <CheckpointDesc SubjectTT="Throttles" ExpectationTT="As required"/>
            <ForceCamera name="Instrument05"/>
            <Instrument Id="ENGINE_LEVER_THROTTLE1"/>
            <Instrument Id="ENGINE_LEVER_THROTTLE2"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ENGINE2_STARTER_BUTTON_PUSH">
            <CheckpointDesc SubjectTT="ENGINE STARTER button" ExpectationTT="Push"/>
            <Clue name="Verify button illuminates"/>
            <Instrument id="ENGINE_Switch_Starter2"/>
            <ForceCamera name="Instrument06"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ENGINE1_STARTER_BUTTON_PUSH">
            <CheckpointDesc SubjectTT="ENGINE STARTER button" ExpectationTT="Push"/>
            <Instrument id="ENGINE_Switch_Starter1"/>
            <Clue name="Verify button illuminates"/>
            <ForceCamera name="Instrument06"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ENGINE2_RUN_STOP_BUTTON_RUN">
