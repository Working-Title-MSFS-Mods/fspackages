            <Instrument Id="LANDING_GEAR_Switch_ParkingBrake"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_PARKING_BRAKE_RELEASE">
            <CheckpointDesc SubjectTT="Parking brake" ExpectationTT="Release"/>
            <ForceCamera name="Instrument04"/>
            <Instrument Id="LANDING_GEAR_Switch_ParkingBrake"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_THROTTLES_IDLE">
            <CheckpointDesc SubjectTT="Throttles" ExpectationTT="IDLE"/>
