        </Checkpoint>
        <Checkpoint Id="CJ4_STBY_FLT_DISPLAY_ON">
            <CheckpointDesc SubjectTT="STBY FLT DISPLAY Switch" ExpectationTT="ON"/>
            <Clue name="Amber light illuminated"/>
            <ForceCamera name="Instrument02"/>
        </Checkpoint>
