        </Checkpoint>
        <Checkpoint Id="CJ4_TRIMS_CHECK_FOR_TO">
            <CheckpointDesc SubjectTT="Trims (3)" ExpectationTT="Set For Takeoff"/>
            <Instrument Id="HANDLING_SWITCH_AILERONTRIM_LR"/>
            <Instrument Id="HANDLING_KNOB_RUDDERTRIM"/>
            <Instrument Id="HANDLING_SWITCH_ELEVATORTRIM"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_FLIGHT_CONTROLS_CHECK">
            <CheckpointDesc SubjectTT="Flight Controls" ExpectationTT="Free/Correct"/>
            <Instrument Id="HANDLING_Yoke"/>
            <Instrument Id="HANDLING_RudderPedals"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_SPEEDBRAKES_GROUND_SPOILERS_CHECK">
            <CheckpointDesc SubjectTT="Speedbrakes/Ground spoilers" ExpectationTT="Check"/>
            <Instrument Id="HANDLING_LEVER_SPOILERS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_GROUND_SPOILERS_ENGAGE">
            <CheckpointDesc SubjectTT="Ground Spoilers" ExpectationTT="Extend"/>
            <Instrument Id="HANDLING_LEVER_SPOILERS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
