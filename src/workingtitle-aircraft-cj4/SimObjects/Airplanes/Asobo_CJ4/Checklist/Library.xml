            <CheckpointDesc SubjectTT="Speed brakes" ExpectationTT="0%"/>
            <Instrument Id="HANDLING_LEVER_SPOILERS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_FLAPS_CHECKSET">
            <CheckpointDesc SubjectTT="Flaps" ExpectationTT="Check/Set"/>
            <Instrument Id="HANDLING_LEVER_FLAPS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_FLAPS_SET_CLIMB">
            <CheckpointDesc SubjectTT="Flaps" ExpectationTT="0 deg"/>
            <Clue name="Clear of obstacles and V2+10"/>
            <Instrument Id="HANDLING_LEVER_FLAPS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_FLAPS_SET_0">
            <CheckpointDesc SubjectTT="Flaps" ExpectationTT="0 deg"/>
            <Instrument Id="HANDLING_LEVER_FLAPS"/>
            <ForceCamera name="Instrument05"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_FLAPS_SET_15">
