            <CheckpointDesc SubjectTT="Aileron Trim" ExpectationTT="Neutral"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_RUDDER_TRIM_NEUTRAL">
            <ForceCamera name="Instrument01"/>
            <CheckpointDesc SubjectTT="Rudder Trim" ExpectationTT="Neutral"/>
        </Checkpoint>
        <Checkpoint Id="CJ4_ELEVATOR_TRIM_TO">
            <ForceCamera name="Instrument01"/>
            <CheckpointDesc SubjectTT="Elevator Trim" ExpectationTT="Set for Takeoff"/>
            <Clue name="Indicator within TO band"/>
        </Checkpoint>

        <!--  -->
        <!-- ROLEPLAY -->
        <!--  -->
