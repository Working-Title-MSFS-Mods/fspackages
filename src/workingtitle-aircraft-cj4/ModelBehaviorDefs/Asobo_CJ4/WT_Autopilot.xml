					<ANIM_CODE>(A:AUTOPILOT FLIGHT DIRECTOR ACTIVE, Bool) 100 *</ANIM_CODE>
				</True>
			</Condition>
		</DefaultTemplateParameters>
		<EditableTemplateParameters>
			<AddParams>
				<ID></ID>
					<FD_TOGGLE_CONDITION></FD_TOGGLE_CONDITION>
			</AddParams>
			<AddOptions>
				<TOGGLEABLE/>
			</AddOptions>
		</EditableTemplateParameters>
		
		<UseTemplate Name="ASOBO_AUTOPILOT_Push_Template">
